{
  "name": "capacitor-plugin-vk-auth",
  "version": "3.0.0-rc.3",
  "description": "VK Authentication with capacitor",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "BazuMax Studio (Maxim Bazuev)",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^3.0.0-rc.0",
    "@capacitor/core": "^3.0.0-rc.0",
    "@capacitor/ios": "^3.0.0-rc.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "~1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "eslint": "^7.11.0",
    "prettier": "~2.2.0",
    "prettier-plugin-java": "~1.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.29.0",
    "swiftlint": "^1.0.1",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "@capacitor/core": "^3.0.0-rc.0"
  },
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CapacitorPluginVkAuth.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "vk",
    "auth"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BazuMax/capacitor-plugin-vk-auth.git"
  },
  "bugs": {
    "url": "https://github.com/BazuMax/capacitor-plugin-vk-auth/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
